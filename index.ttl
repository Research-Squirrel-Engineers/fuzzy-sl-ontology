    rdfs:subClassOf <http://fuzzy-sl.squirrel.link/ontology/Type> .

<http://fuzzy-sl.squirrel.link/ontology/CertaintyType> a owl:Class ;
    rdfs:label "Certainty Type"@en ;
    rdfs:subClassOf <http://fuzzy-sl.squirrel.link/ontology/Type> .

<http://fuzzy-sl.squirrel.link/ontology/SourceType> a owl:Class ;
    rdfs:label "Source Type"@en ;
    rdfs:subClassOf <http://fuzzy-sl.squirrel.link/ontology/Type> .

<http://fuzzy-sl.squirrel.link/ontology/Type> a owl:Class ;
    rdfs:label "Type"@en .

<http://fuzzy-sl.squirrel.link/ontology/Source> a owl:Class ;
    rdfs:label "Source"@en .

<http://fuzzy-sl.squirrel.link/ontology/Place> a owl:Class ;
    rdfs:label "Place"@en ;
