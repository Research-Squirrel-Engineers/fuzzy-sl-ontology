<http://fuzzy-sl.squirrel.link/ontology/activityDesc> a owl:DatatypeProperty ;
    rdfs:label "activity method description"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range xsd:string .

<http://fuzzy-sl.squirrel.link/ontology/certaintyDesc> a owl:DatatypeProperty ;
    rdfs:label "certainty description"@en ;
    rdfs:subPropertyOf owl:topDataProperty .

<http://fuzzy-sl.squirrel.link/ontology/certaintyLevel> a owl:ObjectProperty ;
    rdfs:label "certainty level"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/CertaintyType> .

<http://fuzzy-sl.squirrel.link/ontology/dubiousMatch> a owl:ObjectProperty ;
    rdfs:label "has a dubious match"@en ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skos:definition "the match is dubious (e.g. ambiguity)"@en .

<http://fuzzy-sl.squirrel.link/ontology/georeferencingBy> a owl:ObjectProperty ;
    rdfs:label "georeferencing done by"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range foaf:Person .

<http://fuzzy-sl.squirrel.link/ontology/hasReference> a owl:DatatypeProperty ;
    rdfs:label "has reference"@en .

<http://fuzzy-sl.squirrel.link/ontology/hasSourceType> a owl:ObjectProperty ;
    rdfs:label "has source type"@en ;
    rdfs:subPropertyOf <http://fuzzy-sl.squirrel.link/ontology/hasSource> .

