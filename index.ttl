    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/MethodType> .

<http://fuzzy-sl.squirrel.link/ontology/partOf> a owl:ObjectProperty ;
    rdfs:label "part of"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/Project> .

<http://fuzzy-sl.squirrel.link/ontology/partlyMatch> a owl:ObjectProperty ;
    rdfs:label "has partly match"@en ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skos:definition "is used to state an associative mapping link between two places, in which one is part of another"@en .

<http://fuzzy-sl.squirrel.link/ontology/relatedTo> a owl:ObjectProperty ;
    rdfs:label "related to"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range owl:Thing .

<http://fuzzy-sl.squirrel.link/ontology/representativeGeometry> a owl:ObjectProperty ;
    rdfs:label "representative Geometry"@en ;
    rdfs:comment "e.g. centre point"@en ;
    rdfs:subPropertyOf geo:hasGeometry .

<http://fuzzy-sl.squirrel.link/ontology/siteType> a owl:ObjectProperty ;
    rdfs:label "has site type"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/SiteType> .

<http://fuzzy-sl.squirrel.link/ontology/spatialCloseMatch> a owl:ObjectProperty ;
    rdfs:label "has a spatial close match"@en ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skos:definition "is used to state an associative mapping link between two places, which are spatially close to each other"@en .

<http://fuzzy-sl.squirrel.link/ontology/spatialType> a owl:ObjectProperty ;
    rdfs:label "has spatial type"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/SpatialType> .

