<http://fuzzy-sl.squirrel.link/ontology/hasSource> a owl:ObjectProperty ;
    rdfs:label "has source"@en ;
    rdfs:domain <http://fuzzy-sl.squirrel.link/ontology/Place> ;
    rdfs:range <http://fuzzy-sl.squirrel.link/ontology/Source> .

<http://fuzzy-sl.squirrel.link/ontology/Project> a owl:Class ;
